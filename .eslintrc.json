{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "commonjs"
	},
	"globals": {
		"jQuery": "readonly",
		"mediaWiki": "readonly",
		"nsfr": "readonly",
		"enhancedUpload": "readonly",
		"QUnit": "readonly"
	},
	"rules": {
		"es-x/no-object-entries": "off",
		"max-len": "off",
		"no-jquery/no-done-fail": "warn"
	}
}
